---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="es">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
		<style type="text/css">
			.about-header {
				position: relative;
				padding-top: 20px;
				padding-bottom: 20px;
				height: 50%;
			}
			.about-background {
				background-color: rgb(248, 248, 248);
				width: 100%;
				position: absolute;
				height: 90%;
				border: solid 1px rgb(235, 235, 235);
				z-index: -1;
			}
			.wrapper {
				display: flex;
				max-width: 950px;
				margin: 0 auto;
			}
			.index-header {
				padding: 25px 10px 10px 10px;
				width: 75%;
			}
			.about-img {
				text-align: right;
				position: relative;
				top: -8px;
				padding-right: 10px;

				& img {
					box-shadow: 2px 2px 6px 0px rgba(var(--gray-dark), 85%);
					width: 80%;
				}
			}
			.about-role {
				color: var(--highlight);
			}
			.about-description {
				font-size: 13px;
				color: rgb(104, 104, 104);
			}
			.whisper {
				display: block;
				text-align: right;
				font-size: x-small;
				font-style: oblique;
				color: rgb(var(--gray-light));
			
				&::before {
					font-style: normal;
					content: 'ü§´';
				}
			}
			.about-technologies {
				display: grid;
				grid-template-columns: repeat(
					auto-fill, 
					minmax(200px, 1fr)
				);
			}
			.about-backend {
				padding: 25px 55px 0px 0px;
			}

			@media (width < 600px) {
				.wrapper {
					display: grid;
					grid-template-rows: max-content;
				}
				.index-header {
					grid-row: 2; /* move row to position 2 on media screen */
					width: auto;
					padding: 5px 10px 5px 10px;
				}
				.about-img {
					text-align: center;
					grid-row: 1; /* move row to position 1 on media screen */
					top: -15px;
					& img {
						border-radius: 150px;
						width: 50%;
					}
				}
			}
		</style>
	</head>
